<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 체크박스 해제상태 -->
    <item android:state_checked="false">
    <layer-list>
        <item android:width="@dimen/checkbox_w" android:height="@dimen/checkbox_h" android:drawable="@drawable/incorrect"/>
    </layer-list>
    </item>
    <!-- 체크박스 선택상태 -->
    <item android:state_checked="true">
        <layer-list>
            <item android:width="@dimen/checkbox_w" android:height="@dimen/checkbox_h" android:drawable="@drawable/correct"/>
        </layer-list>
    </item>


</selector>